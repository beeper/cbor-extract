{
  "name": "cbor-extract",
  "author": "Kris Zyp",
  "version": "2.2.1",
  "description": "(Fork) Node addon for string extraction for cbor-x",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/beeper/cbor-extract"
  },
  "scripts": {
    "install": "node-gyp-build-optional-packages",
    "recompile": "node-gyp rebuild",
    "before-publish": "prebuildify-ci download && node set-optional-deps.cjs",
    "prebuild": "prebuildify-platform-packages --target 20.19.4",
    "prebuild-win32": "prebuildify-platform-packages --target 20.19.4 && set ENABLE_V8_FUNCTIONS=false&& prebuildify-platform-packages --platform-packages --napi --target 20.19.4",
    "prebuild-macos": "prebuildify-platform-packages --target 20.0.0 && ENABLE_V8_FUNCTIONS=false prebuildify-platform-packages --napi --platform-packages --target 20.19.4",
    "prebuild-libc": "prebuildify-platform-packages --tag-libc --target 20.19.4 && prebuildify-platform-packages --platform-packages --napi --tag-libc --target 20.19.4 && ENABLE_V8_FUNCTIONS=false prebuildify-platform-packages --platform-packages --napi --tag-libc --target 20.19.4",
    "prebuild-libc-alpine": "prebuildify-cross --image alpine --tag-libc --target 20.19.4",
    "publish-all": "cd prebuilds/win32-x64 && npm publish --access public && cd ../darwin-x64 && npm publish --access public && cd ../darwin-arm64 && npm publish --access public && cd ../linux-x64 && npm publish --access public && cd ../linux-arm64 && npm publish --access public  && cd ../linux-arm && npm publish --access public && cd ../.. && npm publish --access public",
    "test": "node ./index.js"
  },
  "main": "./index.js",
  "gypfile": true,
  "dependencies": {
    "node-gyp-build-optional-packages": "5.2.2"
  },
  "files": [
    "index.js",
    "/src",
    "/*.gyp",
    "/bin"
  ],
  "bin": {
    "download-cbor-prebuilds": "./bin/download-prebuilds.js"
  },
  "devDependencies": {
    "node-gyp": "11.2.0",
    "prebuildify-ci": "1.0.5",
    "prebuildify-cross": "5.1.1",
    "prebuildify-platform-packages": "5.0.5"
  },
  "optionalDependencies": {
    "@cbor-extract/cbor-extract-darwin-arm64": "2.2.0",
    "@cbor-extract/cbor-extract-darwin-x64": "2.2.0",
    "@cbor-extract/cbor-extract-linux-arm": "2.2.0",
    "@cbor-extract/cbor-extract-linux-arm64": "2.2.0",
    "@cbor-extract/cbor-extract-linux-x64": "2.2.0",
    "@cbor-extract/cbor-extract-win32-x64": "2.2.0"
  },
  "packageManager": "yarn@4.9.2+sha512.1fc009bc09d13cfd0e19efa44cbfc2b9cf6ca61482725eb35bbc5e257e093ebf4130db6dfe15d604ff4b79efd8e1e8e99b25fa7d0a6197c9f9826358d4d65c3c"
}
